{
  "name": "blueprint-electron",
  "version": "0.1.0",
  "description": "Electron Blueprint",
  "main": "./out-tsc/main.js",
  "repository": "https://mdk-bs-dev02.test-server.ag/blueprints/blueprint-electron",
  "author": "handschu <sven.handschumacher@adesso.de>",
  "license": "UNLICENSED",
  "scripts": {
    "start": "yarn run electron:rebuild && yarn run build && yarn run start:node",
    "start:node": "./node_modules/.bin/electron .",
    "build": "yarn run build:node && yarn run build:app",
    "build:app": "ng build --prod --output-hashing none --named-chunks true --base-href \"\"",
    "build:node": "tsc -p ./src/tsconfig.app.json",
    "electron:rebuild": "./node_modules/.bin/electron-rebuild",
    "electron:package:win": "yarn build && ./node_modules/.bin/electron-packager . --platform=win32 --overwrite",
    "electron:package:mac": "yarn build && ./node_modules/.bin/electron-packager . --platform=darwin --overwrite",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/cli": "1.3.2",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-beta.3",
    "@paulcbetts/cld": "^2.4.6",
    "@types/sqlite3": "^3.1.1",
    "bootstrap": "next",
    "core-js": "^2.4.1",
    "electron": "^1.7.6",
    "electron-hunspell": "^0.0.3",
    "electron-spellchecker-static-dictionary": "^0.2.3",
    "font-awesome": "^4.7.0",
    "ng2-ckeditor": "^1.1.9",
    "ngx-electron": "1.0.3",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.6",
    "sqlite3": "^3.1.13",
    "typeorm": "^0.1.1",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^8.0.47",
    "codelyzer": "~3.1.1",
    "concurrently": "^3.5.0",
    "electron-packager": "^10.1.2",
    "electron-rebuild": "^1.6.0",
    "global": "^4.3.2",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-annotate-loader": "^0.6.1",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3",
    "webpack-config-file": "^1.0.5"
  }
}
